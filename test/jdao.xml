<?xml version="1.0" encoding="gbk"?>
<jdao-table>
	<!-- field属性type对应的选项  -->
	<!--
		type : int , String , BigDecimal , Date , byte[] , boolean
	-->
	<!-- type值对应 表字段类型 -->
	<!--  int        :  TINYINT, INTEGER, SMALLINT -->
	<!--  String     :  CHAR, VARCHAR, LONGNVARCHAR  -->
	<!--  bigDecimal :  DECIMAL, NUMERIC, DOUBLE, FLOAT, REAL, BIGINT   -->
	<!--  Date       :  TIMESTAMP, DATE, TIME -->
	<!--  byte[]     :  LONGVARBINARY, BINARY, VARBINARY -->
	<!--  boolean    :  BIT -->



	<table name="hstest" package="com.jdao.dao">
		<field name="id" type="int" />
		<field name="rowname" type="String" />
		<field name="value" type="String" />
	</table>

	<table name="hstest2">
		<field name="id" type="int" />
		<field name="rowname" type="String" />
		<field name="income" type="BigDecimal" />
		<field name="day" type="Date" />
	</table>

</jdao-table>